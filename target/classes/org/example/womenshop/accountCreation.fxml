<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.womenshop.AccountCreation">
    <children>
        <Label layoutX="50.0" layoutY="50.0" text="First Name:" />
        <TextField fx:id="firstNameField" layoutX="150.0" layoutY="50.0" prefHeight="26.0" prefWidth="200.0" />

        <Label layoutX="50.0" layoutY="100.0" text="Last Name:" />
        <TextField fx:id="lastNameField" layoutX="150.0" layoutY="100.0" prefHeight="26.0" prefWidth="200.0" />

        <Label layoutX="50.0" layoutY="150.0" text="Email:" />
        <TextField fx:id="emailField" layoutX="150.0" layoutY="150.0" prefHeight="26.0" prefWidth="200.0" />

        <Label layoutX="50.0" layoutY="200.0" text="Password:" />
        <PasswordField fx:id="passwordField" layoutX="150.0" layoutY="200.0" prefHeight="26.0" prefWidth="200.0" />

        <Label layoutX="50.0" layoutY="250.0" text="Confirm Password:" />
        <PasswordField fx:id="confirmPasswordField" layoutX="150.0" layoutY="250.0" prefHeight="26.0" prefWidth="200.0" />

        <Button layoutX="150.0" layoutY="300.0" mnemonicParsing="false" onAction="#onCreateAccountButtonClick" text="Create Account" />
    </children>
</AnchorPane>